{
    "name": "icecave/evoke",
    "description": "A library for Invoking PHP callables using positional and named arguments.",
    "keywords": ["function", "keyword", "arguments", "parameters", "invoke", "call", "callable"],
    "homepage": "https://github.com/IcecaveStudios/evoke",
    "license": "MIT",
    "authors": [
        {
            "name": "James Harris",
            "email": "james.harris@icecave.com.au",
            "homepage": "https://github.com/jmalloc"
        }
    ],
    "require": {
        "php": ">=5.3"
    },
    "require-dev": {
        "eloquent/typhoon": "~0.9.0",
        "icecave/archer": "~1"
    },
    "suggest": {
        "eloquent/asplode": "Drop-in exception-based error handling."
    },
    "autoload": {
        "psr-0": {
            "Icecave\\Evoke": "src",
            "Icecave\\Evoke\\TypeCheck": "src-typhoon"
        }
    },
    "extra": {
        "typhoon": {
            "output-path": "src-typhoon",
            "validator-namespace": "Icecave\\Evoke\\TypeCheck",
            "use-native-callable": false
        }
    }
}
