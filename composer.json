{
    "name": "icecave/evoke",
    "description": "A library for invoking PHP callables using positional and named arguments.",
    "keywords": [
        "function",
        "keyword",
        "arguments",
        "parameters",
        "invoke",
        "call",
        "callable"
    ],
    "homepage": "https://github.com/IcecaveStudios/evoke",
    "license": "MIT",
    "authors": [
        {
            "name": "James Harris",
            "email": "james.harris@icecave.com.au",
            "homepage": "https://github.com/jmalloc"
        }
    ],
    "require": {
        "php": ">=5.3"
    },
    "require-dev": {
        "icecave/archer": "~1"
    },
    "autoload": {
        "psr-4": {
            "Icecave\\Evoke\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        }
    }
